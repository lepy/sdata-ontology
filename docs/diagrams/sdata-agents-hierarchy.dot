digraph "" {
	graph [bgcolor="#FAFBFC",
		fontname=Helvetica,
		fontsize=20,
		label="sdata Agents Hierarchy (core agents -> SKOS classifications)",
		labeljust=c,
		labelloc=t,
		nodesep=0.55,
		overlap=false,
		pad=0.35,
		rankdir=TB,
		ranksep=1.0,
		splines=true
	];
	node [fontname=Helvetica,
		fontsize=12,
		label="\N",
		margin="0.14,0.08",
		penwidth=1.6,
		shape=box,
		style="filled,rounded"
	];
	edge [arrowsize=0.85,
		color="#4A5568",
		fontname=Helvetica,
		penwidth=1.6
	];
	subgraph cluster_legend {
		graph [color="#CBD5E0",
			label=Legend,
			style=rounded
		];
		legend_sdata	[color="#2B6CB0",
			fillcolor="#E8F1FF",
			fontcolor="#1A365D",
			label="sdata agent class"];
		legend_scheme	[color="#2F855A",
			fillcolor="#E7FFF6",
			fontcolor="#1C4532",
			label="SKOS ConceptScheme"];
		legend_sdata -> legend_scheme	[label="classification scheme"];
		legend_concept	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label="SKOS Concept"];
		legend_scheme -> legend_concept	[label="broader -> narrower"];
	}
	subgraph cluster_core {
		graph [color="#90B5E8",
			label="sdata-core ontology",
			style=rounded
		];
		"https://w3id.org/sdata/core/Agent"	[color="#2B6CB0",
			fillcolor="#E8F1FF",
			fontcolor="#1A365D",
			label=Agent];
		__anchor_core	[height=0,
			label="",
			style=invis,
			width=0];
	}
	subgraph cluster_agents {
		graph [color="#9AD9BF",
			label="sdata-agents ontology",
			style=rounded
		];
		"https://w3id.org/sdata/vocab/agents/Actuator"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Actuator];
		"https://w3id.org/sdata/vocab/agents/AgentTypeScheme"	[color="#2F855A",
			fillcolor="#E7FFF6",
			fontcolor="#1C4532",
			label="Agent types"];
		"https://w3id.org/sdata/vocab/agents/Hardware"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Hardware];
		"https://w3id.org/sdata/vocab/agents/Organization"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Organization];
		"https://w3id.org/sdata/vocab/agents/Person"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Person];
		"https://w3id.org/sdata/vocab/agents/Processor"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Processor];
		"https://w3id.org/sdata/vocab/agents/Sensor"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Sensor];
		"https://w3id.org/sdata/vocab/agents/Service"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Service];
		"https://w3id.org/sdata/vocab/agents/Software"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Software];
		"https://w3id.org/sdata/vocab/agents/Solver"	[color="#2F855A",
			fillcolor="#F0FFF9",
			fontcolor="#1C4532",
			label=Solver];
		__anchor_agents	[height=0,
			label="",
			style=invis,
			width=0];
	}
	"https://w3id.org/sdata/core/Agent" -> "https://w3id.org/sdata/vocab/agents/AgentTypeScheme"	[label="classification scheme"];
	"https://w3id.org/sdata/core/Agent" -> "https://w3id.org/sdata/vocab/agents/Hardware"	[label="also an"];
	"https://w3id.org/sdata/core/Agent" -> "https://w3id.org/sdata/vocab/agents/Organization"	[label="also an"];
	"https://w3id.org/sdata/core/Agent" -> "https://w3id.org/sdata/vocab/agents/Person"	[label="also an"];
	"https://w3id.org/sdata/core/Agent" -> "https://w3id.org/sdata/vocab/agents/Software"	[label="also an"];
	"https://w3id.org/sdata/vocab/agents/AgentTypeScheme" -> "https://w3id.org/sdata/vocab/agents/Hardware";
	"https://w3id.org/sdata/vocab/agents/AgentTypeScheme" -> "https://w3id.org/sdata/vocab/agents/Organization";
	"https://w3id.org/sdata/vocab/agents/AgentTypeScheme" -> "https://w3id.org/sdata/vocab/agents/Person";
	"https://w3id.org/sdata/vocab/agents/AgentTypeScheme" -> "https://w3id.org/sdata/vocab/agents/Software";
	"https://w3id.org/sdata/vocab/agents/Hardware" -> "https://w3id.org/sdata/vocab/agents/Actuator";
	"https://w3id.org/sdata/vocab/agents/Hardware" -> "https://w3id.org/sdata/vocab/agents/Processor";
	"https://w3id.org/sdata/vocab/agents/Hardware" -> "https://w3id.org/sdata/vocab/agents/Sensor";
	"https://w3id.org/sdata/vocab/agents/Software" -> "https://w3id.org/sdata/vocab/agents/Service";
	"https://w3id.org/sdata/vocab/agents/Software" -> "https://w3id.org/sdata/vocab/agents/Solver";
	__anchor_core -> __anchor_agents	[style=invis,
		weight=20];
}
