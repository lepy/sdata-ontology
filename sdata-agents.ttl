# =============================================================================
# FILE: sdata-agents.ttl
# Controlled vocabulary for sdata:agentType
# Aligned with MaterialAgent / InformationAgent duality
# =============================================================================

@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

@prefix sdata: <https://w3id.org/sdata/core#> .
@prefix sagents:  <https://w3id.org/sdata/vocab/agents#> .

# =============================================================================
# Ontology header
# =============================================================================

<https://w3id.org/sdata/vocab/agents>
    a owl:Ontology ;
    owl:imports <https://w3id.org/sdata/core> ;
    owl:versionInfo "0.2.0 — 27 February 2026" ;
    rdfs:label "sdata agentType vocabulary"@en ;
    rdfs:label "sdata agentType-Vokabular"@de ;
    dcterms:description
        """Controlled vocabulary (SKOS) for sdata:agentType. Mirrors the
5+5+1 duality: Material agents (Person, Machine) are tangible,
Information agents (Organization, Solver, Service, DigitalTwin) are
intangible."""@en .

# =============================================================================
# ConceptScheme
# =============================================================================

sagents:AgentTypeScheme
    a skos:ConceptScheme ;
    skos:prefLabel "Agent types"@en ;
    skos:prefLabel "Agent-Typen"@de ;
    skos:hasTopConcept
        sagents:Person ,
        sagents:Machine ,
        sagents:Organization ,
        sagents:Software
.

# =============================================================================
# MATERIAL AGENTS — tangible, you can point at them
# =============================================================================

sagents:Person
    a skos:Concept ;
    skos:inScheme sagents:AgentTypeScheme ;
    skos:topConceptOf sagents:AgentTypeScheme ;
    skos:prefLabel "Person"@en , "Person"@de ;
    skos:definition "Human agent. Instance of sdata:MaterialAgent."@en ;
    skos:scopeNote "Identify via ORCID (researchers), or name + affiliation."@en .

sagents:Machine
    a skos:Concept ;
    skos:inScheme sagents:AgentTypeScheme ;
    skos:topConceptOf sagents:AgentTypeScheme ;
    skos:prefLabel "Machine"@en , "Maschine"@de ;
    skos:definition "Complex material system with sensors and actuators. Instance of sdata:MaterialAgent."@en ;
    skos:scopeNote "Identify via serial number, asset tag, or DPP identifier."@en ;
    skos:example "Test machine (Zwick Z100), robot arm, production line, sensor."@en .

# =============================================================================
# INFORMATION AGENTS — intangible, you cannot point at them
# =============================================================================

sagents:Organization
    a skos:Concept ;
    skos:inScheme sagents:AgentTypeScheme ;
    skos:topConceptOf sagents:AgentTypeScheme ;
    skos:prefLabel "Organization"@en , "Organisation"@de ;
    skos:definition "Social entity constituted by agreements, structures, and roles. Instance of sdata:InformationAgent. Depends on material carriers (people, buildings) but is not identical with any of them."@en ;
    skos:scopeNote "Identify via LEI, DUNS, VAT-ID, or ROR (research orgs)."@en ;
    skos:example "Fraunhofer IWM, BMW Group, DIN e.V."@en .

sagents:Software
    a skos:Concept ;
    skos:inScheme sagents:AgentTypeScheme ;
    skos:topConceptOf sagents:AgentTypeScheme ;
    skos:prefLabel "Software"@en , "Software"@de ;
    skos:definition "Executable information entity acting as agent. Instance of sdata:InformationAgent."@en ;
    skos:scopeNote "Identify via URI + version string, or package hash (SHA-256)."@en ;
    skos:narrower
        sagents:Solver ,
        sagents:Service ,
        sagents:DigitalTwin .

sagents:Solver
    a skos:Concept ;
    skos:inScheme sagents:AgentTypeScheme ;
    skos:broader sagents:Software ;
    skos:prefLabel "Solver"@en , "Solver"@de ;
    skos:definition "Software that computes a model of a physical or logical system (FEM solver, CFD solver, ML runtime). The solver is the *agent* that performs an Observation process (simulation run)."@en ;
    skos:example "LS-DYNA R14, Abaqus 2024, OpenFOAM v12, PyTorch Runtime."@en ;
    skos:historyNote "Renamed from sagents:Simulation in v1.1.0 to avoid ambiguity with the verb class sdata:Observation."@en .

sagents:Service
    a skos:Concept ;
    skos:inScheme sagents:AgentTypeScheme ;
    skos:broader sagents:Software ;
    skos:prefLabel "Service"@en , "Dienst"@de ;
    skos:definition "Persistent software endpoint that transforms, validates, or stores data (API, ETL pipeline, data broker)."@en ;
    skos:example "REST API, SPARQL endpoint, CI/CD pipeline, message broker."@en .

sagents:DigitalTwin
    a skos:Concept ;
    skos:inScheme sagents:AgentTypeScheme ;
    skos:broader sagents:Software ;
    skos:prefLabel "Digital Twin"@en , "Digitaler Zwilling"@de ;
    skos:definition "Software representation of a material entity that synchronizes state bidirectionally and may act autonomously."@en ;
    skos:scopeNote "A Digital Twin is both data consumer and producer. References its material counterpart via sdata:hasIdentifier."@en .

# =============================================================================
# MAPPING — agentType value to Core class
# =============================================================================
#
# agentType value     Core class              Tangible?
# ──────────────────  ──────────────────────  ─────────
# Person              sdata:MaterialAgent      ✓
# Machine             sdata:MaterialAgent      ✓
# Organization        sdata:InformationAgent   ✗
# Software            sdata:InformationAgent   ✗
#   └── Solver        sdata:InformationAgent   ✗
#   └── Service       sdata:InformationAgent   ✗
#   └── DigitalTwin   sdata:InformationAgent   ✗
#
# =============================================================================
# END — sdata-agents.ttl v0.2.0
# =============================================================================
