    owl:imports      <https://w3id.org/sdata/core> ;
    owl:imports      <urn:sdata:vendor:bfo> ;
    owl:imports      <urn:sdata:vendor:prov> ;

    # ─── Conformance ─────────────────────────────────────────────────────────
