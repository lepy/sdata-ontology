#     sdata:fuzzyPeak          "303.0"^^xsd:double ;
#     sdata:fuzzyRight         "320.0"^^xsd:double ;
#     sdata:domainDescription  "Approximate tensile strength DC04" .
#
# ex:attr_Rm  a  sdata:AttributeQuantityValue ;
#     sdata:name           "Rm" ;
#     sdata:dtype          "float" ;
#     qudt:numericValue    "303.0"^^xsd:double ;
#     sdata:unitSymbol     "MPa" ;
#     sdata:hasValueDomain ex:dom_Rm_fuzzy .
#
# ─── "Thickness tolerance" — trapezoidal ────────────────
# ex:dom_thickness_tol  a  sdata:TrapezoidalFuzzyDomain ;
#     sdata:fuzzyLeft          "0.92"^^xsd:double ;
#     sdata:fuzzyCoreLeft      "0.98"^^xsd:double ;
#     sdata:fuzzyCoreRight     "1.02"^^xsd:double ;
#     sdata:fuzzyRight         "1.08"^^xsd:double ;
#     sdata:domainDescription  "Sheet thickness tolerance band (mm)" .
#
# ─── "Sensor noise" — Gaussian ──────────────────────────
# ex:dom_sensor_noise  a  sdata:GaussianFuzzyDomain ;
#     sdata:fuzzyCenter        "0.0"^^xsd:double ;
#     sdata:fuzzyWidth         "0.5"^^xsd:double ;
#     sdata:domainDescription  "Sensor measurement noise model" .
#
