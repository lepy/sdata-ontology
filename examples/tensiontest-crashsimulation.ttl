@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sdata: <https://w3id.org/sdata/core/> .
@prefix ex:    <https://example.org/simulation/> .

ex:Specimen_42
    a sdata:Specimen ;
    sdata:hasIdentifier "SPEC-42" .

ex:ZwickZ100
    a sdata:MachineAgent ;
    sdata:hasIdentifier "MCH-ZWICK-Z100" .

ex:LSDYNA_R14
    a sdata:SoftwareAgent ;
    sdata:hasIdentifier "SW-LSDYNA-R14" .

ex:TensileTest_42
    a sdata:MechanicalTest ;
    sdata:hasInput ex:Specimen_42 ;
    sdata:performedBy ex:ZwickZ100 ;
    sdata:producesData ex:MeasurementData_42 ;
    sdata:hasStandard "DIN EN ISO 6892-1" .

ex:CrashSim_v3
    a sdata:CrashSimulation ;
    sdata:hasInput ex:FEModel_v3 ;
    sdata:performedBy ex:LSDYNA_R14 ;
    sdata:producesData ex:SimulationResult_v3 .

ex:MeasurementData_42
    a sdata:MeasurementData ;
    sdata:describes ex:Specimen_42 ;
    sdata:producedBy ex:TensileTest_42 ;
    sdata:hasValue 18.5 ;
    sdata:hasUnit "percent" ;
    sdata:hasVersion "1.0.0" .

ex:FEModel_v3
    a sdata:Model ;
    sdata:describes ex:Specimen_42 ;
    sdata:hasVersion "3.0.0" .

ex:SimulationResult_v3
    a sdata:SimulationResult ;
    sdata:describes ex:Specimen_42 ;
    sdata:producedBy ex:CrashSim_v3 ;
    sdata:hasVersion "3.0.0" .
