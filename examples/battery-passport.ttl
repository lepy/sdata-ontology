@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix sdata: <https://w3id.org/sdata/core/> .
@prefix ex:    <https://example.org/battery/> .

# =============================================================================
# Example: Battery Pack Product Passport (sdata core 0.6.0)
# =============================================================================

# --- Objects -----------------------------------------------------------------

ex:SteelCoil_001
    a sdata:SemiFinishedProduct ;
    sdata:hasIdentifier "COIL-001" .

ex:DoorBeam_001
    a sdata:Component ;
    sdata:hasIdentifier "PART-DOORBEAM-001" .

ex:DoorModule_001
    a sdata:Assembly ;
    sdata:hasIdentifier "ASM-DOOR-001" .

ex:BatteryPack_001
    a sdata:Product, sdata:Object ;
    sdata:hasIdentifier "VP-BAT-2026-000001" .

ex:DC04
    a sdata:Material ;
    sdata:hasIdentifier "MAT-DC04" .

ex:Zugprobe_A1
    a sdata:Specimen ;
    sdata:hasIdentifier "SPEC-A1" .

ex:TensileTool
    a sdata:Tool ;
    sdata:hasIdentifier "TOOL-TENSILE-01" .

ex:RecycledSteel_Batch_7
    a sdata:Recyclate ;
    sdata:hasIdentifier "REC-STEEL-007" .

# --- Agents ------------------------------------------------------------------

ex:OperatorMueller
    a sdata:HumanAgent ;
    sdata:hasIdentifier "HUM-042" .

ex:PressLine_A1
    a sdata:MachineAgent ;
    sdata:hasIdentifier "MCH-PRESS-A1" .

ex:LSDYNA_R14
    a sdata:SoftwareAgent ;
    sdata:hasIdentifier "SW-LSDYNA-R14" .

ex:VoltPackAG
    a sdata:Organization, sdata:Agent ;
    sdata:hasIdentifier "ORG-VOLTPACK" .

# --- Activities ---------------------------------------------------------------

ex:StampingRun_01
    a sdata:FormingProcess, sdata:Activity ;
    sdata:hasInput ex:SteelCoil_001 ;
    sdata:hasOutput ex:DoorBeam_001 ;
    sdata:performedBy ex:PressLine_A1 ;
    sdata:usesTool ex:TensileTool ;
    sdata:producesData ex:StampingData_01 ;
    sdata:hasStandard "DIN 8584-3" .

ex:TensileTest_A1
    a sdata:MechanicalTest ;
    sdata:hasInput ex:Zugprobe_A1 ;
    sdata:performedBy ex:OperatorMueller ;
    sdata:performedBy ex:PressLine_A1 ;
    sdata:usesTool ex:TensileTool ;
    sdata:producesData ex:TensileResult_A1 ;
    sdata:hasStandard "DIN EN ISO 6892-1" .

ex:CrashSimulation_v3
    a sdata:Simulation ;
    sdata:hasInput ex:BatteryModel_v3 ;
    sdata:performedBy ex:LSDYNA_R14 ;
    sdata:producesData ex:CrashResult_v3 ;
    sdata:hasStandard "OEM Crash Protocol 3.2" .

ex:PackAssembly_01
    a sdata:AssemblyProcess ;
    sdata:hasInput ex:DoorBeam_001 ;
    sdata:hasOutput ex:DoorModule_001 ;
    sdata:performedBy ex:OperatorMueller ;
    sdata:producesData ex:AssemblyData_01 .

ex:SteelRecovery_01
    a sdata:RecyclingProcess ;
    sdata:hasInput ex:DoorBeam_001 ;
    sdata:hasOutput ex:RecycledSteel_Batch_7 ;
    sdata:performedBy ex:VoltPackAG ;
    sdata:producesData ex:RecoveryData_01 .

# --- Data --------------------------------------------------------------------

ex:StampingData_01
    a sdata:ProcessData ;
    sdata:describes ex:DoorBeam_001 ;
    sdata:producedBy ex:StampingRun_01 ;
    sdata:hasValue 1850.0 ;
    sdata:hasUnit "kN" ;
    sdata:hasVersion "1.0.0" .

ex:TensileResult_A1
    a sdata:MeasurementData ;
    sdata:describes ex:Zugprobe_A1 ;
    sdata:producedBy ex:TensileTest_A1 ;
    sdata:hasQuantity ex:YieldStrength_AQV ;
    sdata:hasVersion "1.0.0" .

ex:CrashResult_v3
    a sdata:SimulationResult ;
    sdata:describes ex:BatteryPack_001 ;
    sdata:producedBy ex:CrashSimulation_v3 ;
    sdata:hasVersion "3.0.0" .

ex:BatteryModel_v3
    a sdata:Model ;
    sdata:describes ex:BatteryPack_001 ;
    sdata:hasVersion "3.0.0" .

ex:Passport_001
    a sdata:Passport, sdata:Data ;
    sdata:describes ex:BatteryPack_001 ;
    sdata:hasVersion "0.6.0" .

ex:InspectionCertificate_001
    a sdata:Certificate ;
    sdata:describes ex:BatteryPack_001 ;
    sdata:certifiedBy ex:VoltPackAG ;
    sdata:hasStandard "EN 10204 3.1" ;
    sdata:hasVersion "2026-03" .

ex:YieldStrengthData
    a sdata:MaterialProperty ;
    sdata:describes ex:DC04 ;
    sdata:hasValue 340.0 ;
    sdata:hasUnit "MPa" ;
    sdata:hasVersion "1.0.0" .

ex:Meta_StampingData
    a sdata:Metadata ;
    sdata:describes ex:StampingData_01 ;
    sdata:hasVersion "1.0.0" .

ex:MechanicalTestReport
    a sdata:Table ;
    sdata:describes ex:TensileTest_A1 ;
    sdata:hasVersion "1.0.0" .

# --- AQV / ValueDomain -------------------------------------------------------

ex:YieldStrength_AQV
    a sdata:AttributeQuantityValue ;
    sdata:unitSymbol "MPa" ;
    sdata:dtype "float" ;
    sdata:hasValueDomain ex:YieldStrengthSpec .

ex:YieldStrengthSpec
    a sdata:ValueDomain ;
    sdata:domainDescription "Yield strength must be between 270 and 410 MPa." .

# --- Object to Data aggregation ---------------------------------------------

ex:BatteryPack_001
    sdata:hasPassport ex:Passport_001 ;
    sdata:hasData ex:CrashResult_v3 ;
    sdata:hasData ex:InspectionCertificate_001 .

# =============================================================================
# END â€” Battery Passport Example
# =============================================================================
