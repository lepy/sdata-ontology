@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix sdata: <https://w3id.org/sdata/core#> .
@prefix ex: <https://example.org/battery/> .

# =============================================================================
# Example: Battery Pack Product Passport (sdata core 0.2.0)
# =============================================================================

# Material Agents
ex:OperatorMueller
    a sdata:MaterialAgent ;
    sdata:name "Max Mueller" ;
    sdata:agentType "Person" ;
    sdata:locatedAt ex:PackAssembly .

ex:ZwickZ100
    a sdata:MaterialAgent ;
    sdata:name "Zwick Z100" ;
    sdata:agentType "Machine" ;
    sdata:locatedAt ex:PackAssembly .

# Information Agents
ex:VoltPackAG
    a sdata:InformationAgent ;
    sdata:name "VoltPack AG" ;
    sdata:agentType "Organization" ;
    sdata:hostedAt ex:DPPBackend .

ex:LSDYNA_R14
    a sdata:InformationAgent ;
    sdata:name "LS-DYNA R14" ;
    sdata:agentType "Solver" ;
    sdata:hostedAt ex:HPCCluster .

# Material Sites
ex:PackAssembly
    a sdata:MaterialSite ;
    sdata:name "VoltPack Montage Dresden" ;
    sdata:geoCoordinates "51.0504,13.7373" ;
    sdata:address "Tesla Str. 19, 01309 Dresden, Germany" ;
    sdata:containsSite ex:PackAssemblyHall3 .

ex:PackAssemblyHall3
    a sdata:MaterialSite ;
    sdata:name "Halle 3" ;
    sdata:geoCoordinates "51.0506,13.7375" .

# Information Sites
ex:DPPBackend
    a sdata:InformationSite ;
    sdata:name "DPP Backend" ;
    sdata:endpoint "https://dpp.voltpack.example/api"^^xsd:anyURI ;
    sdata:locatedAt ex:PackAssembly ;
    sdata:hostedOn ex:ServerRack01 .

ex:HPCCluster
    a sdata:InformationSite ;
    sdata:name "HPC Cluster" ;
    sdata:endpoint "https://hpc.voltpack.example"^^xsd:anyURI ;
    sdata:locatedAt ex:PackAssembly ;
    sdata:hostedOn ex:ServerRack01 .

# Materials
ex:NMC811Cathode
    a sdata:Material ;
    sdata:name "NMC 811 Kathodenmaterial" ;
    sdata:description "LiNi0.8Mn0.1Co0.1O2" .

ex:RecycledNickel
    a sdata:Material ;
    sdata:name "Recyceltes Nickelsulfat" ;
    sdata:recoveredFrom ex:BatteryPack001 .

# Identifiers
ex:BatteryPack001Serial
    a sdata:Identifier ;
    sdata:identifierType "Serial" ;
    sdata:identifierValue "VP-BAT-2026-000001" ;
    sdata:validFrom "2026-01-15T08:00:00Z"^^xsd:dateTimeStamp .

ex:DPPUriId
    a sdata:Identifier ;
    sdata:identifierType "URI" ;
    sdata:identifierValue "urn:voltpack:dpp:VP-BAT-2026-000001" .

# Material Artifacts
ex:ServerRack01
    a sdata:MaterialArtifact ;
    sdata:name "Server Rack 01" ;
    sdata:hasIdentifier ex:DPPUriId ;
    sdata:locatedAt ex:PackAssembly .

ex:BatteryPack001
    a sdata:MaterialArtifact ;
    sdata:name "VoltPack 96S Battery Pack" ;
    sdata:description "Automotive battery pack" ;
    sdata:hasIdentifier ex:BatteryPack001Serial ;
    sdata:consistsOf ex:NMC811Cathode ;
    sdata:hasPart ex:ModuleM01 ;
    sdata:locatedAt ex:PackAssembly .

ex:ModuleM01
    a sdata:MaterialArtifact ;
    sdata:name "Module M01" ;
    sdata:hasIdentifier ex:BatteryPack001Serial ;
    sdata:locatedAt ex:PackAssemblyHall3 .

# Roles
ex:ManufacturerRole
    a sdata:Role ;
    sdata:roleType "Manufacturer" ;
    sdata:description "Hersteller des Battery Packs" .

ex:ProductPassportRole
    a sdata:Role ;
    sdata:roleType "ProductPassport" .

# Role realizations
ex:VoltPackAG sdata:realizes ex:ManufacturerRole .

# Information Artifacts
ex:BatteryPassport001
    a sdata:InformationArtifact ;
    sdata:name "Battery Passport VP-BAT-2026-000001" ;
    sdata:version "0.2.0" ;
    sdata:description "EU Digital Product Passport" ;
    sdata:represents ex:BatteryPack001 ;
    sdata:realizes ex:ProductPassportRole ;
    sdata:hasIdentifier ex:DPPUriId ;
    sdata:hostedAt ex:DPPBackend ;
    sdata:validFrom "2026-01-15T08:00:00Z"^^xsd:dateTimeStamp .

# Material Process
ex:PackAssemblyProcess
    a sdata:MaterialProcess ;
    sdata:name "Pack-Montage VP-BAT-2026-000001" ;
    sdata:wasPerformedBy ex:OperatorMueller ;
    sdata:consumes ex:NMC811Cathode ;
    sdata:generates ex:BatteryPack001 ;
    sdata:locatedAt ex:PackAssemblyHall3 ;
    prov:startedAtTime "2026-01-15T07:00:00Z"^^xsd:dateTimeStamp ;
    prov:endedAtTime "2026-01-15T15:30:00Z"^^xsd:dateTimeStamp .

# Information Process
ex:CrashSimulationV3
    a sdata:InformationProcess ;
    sdata:name "Crash-Simulation v3" ;
    sdata:wasPerformedBy ex:LSDYNA_R14 ;
    sdata:consumes ex:BatteryPassport001 ;
    sdata:generates ex:SimulationReportV3 ;
    sdata:hostedAt ex:HPCCluster ;
    prov:startedAtTime "2026-02-25T14:00:00Z"^^xsd:dateTimeStamp ;
    prov:endedAtTime "2026-02-25T16:00:00Z"^^xsd:dateTimeStamp .

ex:SimulationReportV3
    a sdata:InformationArtifact ;
    sdata:name "Simulation Report v3" ;
    sdata:version "3.0" ;
    sdata:represents ex:CrashSimulationV3 ;
    sdata:hostedAt ex:HPCCluster .

# =============================================================================
# END â€” Battery Passport Example
# =============================================================================
