@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix sdata: <https://w3id.org/sdata/core#> .
@prefix ex: <https://example.org/battery/> .

# =============================================================================
# Example: Battery Pack Product Passport
# Demonstrates all 8 sdata classes and key relations
# Validate with: pyshacl -s shapes/sdata-core-shapes.ttl -df turtle examples/battery-passport.ttl
# =============================================================================

# ─── Agents ──────────────────────────────────────────────────────────────────
ex:CellChem
    a sdata:Agent ;
    sdata:name "CellChem GmbH" ;
    sdata:agentType "Organization" ;
    sdata:hasIdentifier ex:CellChem-GLN .

ex:VoltPack
    a sdata:Agent ;
    sdata:name "VoltPack AG" ;
    sdata:agentType "Organization" .

ex:RecycleX
    a sdata:Agent ;
    sdata:name "RecycleX OÜ" ;
    sdata:agentType "Organization" .

# ─── Sites ───────────────────────────────────────────────────────────────────
ex:CellFactory
    a sdata:Site ;
    sdata:name "CellChem Werk Erfurt" ;
    sdata:geoCoordinates "50.9787,11.0328" ;
    sdata:address "Industriestr. 42, 99085 Erfurt, Germany" .

ex:PackAssembly
    a sdata:Site ;
    sdata:name "VoltPack Montage Dresden" ;
    sdata:geoCoordinates "51.0504,13.7373" ;
    sdata:address "Tesla Str. 19, 01309 Dresden, Germany" ;
    sdata:containsSite ex:PackAssembly-Hall3 .

ex:PackAssembly-Hall3
    a sdata:Site ;
    sdata:name "Halle 3 — Endmontage" ;
    sdata:geoCoordinates "51.0506,13.7375" .

ex:RecyclingYard
    a sdata:Site ;
    sdata:name "RecycleX Anlage Tallinn" ;
    sdata:geoCoordinates "59.4370,24.7536" ;
    sdata:address "Peterburi tee 71, 11415 Tallinn, Estonia" .

# ─── Materials ───────────────────────────────────────────────────────────────
ex:LithiumCarbonate
    a sdata:Material ;
    sdata:name "Lithiumcarbonat (Li₂CO₃)" ;
    sdata:description "Battery-grade, 99.5% purity" ;
    sdata:hasIdentifier ex:LithiumCarbonate-CAS .

ex:NMC811Cathode
    a sdata:Material ;
    sdata:name "NMC 811 Kathodenmaterial" ;
    sdata:description "LiNi₀.₈Mn₀.₁Co₀.₁O₂" .

ex:RecycledNickel
    a sdata:Material ;
    sdata:name "Recyceltes Nickelsulfat" ;
    sdata:description "Aus Altbatterien zurückgewonnen" ;
    sdata:recoveredFrom ex:BatteryPack-001 .

# ─── Identifiers ─────────────────────────────────────────────────────────────
ex:CellChem-GLN
    a sdata:Identifier ;
    sdata:identifierType "GLN" ;
    sdata:identifierValue "4012345000010" .

ex:LithiumCarbonate-CAS
    a sdata:Identifier ;
    sdata:identifierType "CAS" ;
    sdata:identifierValue "554-13-2" .

ex:BatteryPack-001-Serial
    a sdata:Identifier ;
    sdata:identifierType "Serial" ;
    sdata:identifierValue "VP-BAT-2026-000001" ;
    sdata:validFrom "2026-01-15T08:00:00Z"^^xsd:dateTimeStamp .

ex:BatteryPack-001-GTIN
    a sdata:Identifier ;
    sdata:identifierType "GTIN" ;
    sdata:identifierValue "04012345000027" .

ex:BatteryPack-001-DID
    a sdata:Identifier ;
    sdata:identifierType "DID" ;
    sdata:identifierValue "did:web:voltpack.eu:bat:VP-BAT-2026-000001" .

ex:Cell-A01-Serial
    a sdata:Identifier ;
    sdata:identifierType "Serial" ;
    sdata:identifierValue "CC-CELL-2026-A01" .

# ─── Physical Artifacts ──────────────────────────────────────────────────────
ex:BatteryPack-001
    a sdata:PhysicalArtifact ;
    sdata:name "VoltPack 96S Battery Pack" ;
    sdata:description "96s1p NMC 811 automotive battery pack, 72 kWh" ;
    sdata:hasIdentifier ex:BatteryPack-001-Serial ,
                        ex:BatteryPack-001-GTIN ,
                        ex:BatteryPack-001-DID ;
    sdata:hasPart ex:Module-M01 ;
    sdata:consistsOf ex:NMC811Cathode ;
    sdata:locatedAt ex:PackAssembly .

ex:Module-M01
    a sdata:PhysicalArtifact ;
    sdata:name "Modul M01 (12s1p)" ;
    sdata:hasIdentifier ex:Cell-A01-Serial ;
    sdata:hasPart ex:Cell-A01 .

ex:Cell-A01
    a sdata:PhysicalArtifact ;
    sdata:name "Prismatic Cell A01" ;
    sdata:hasIdentifier ex:Cell-A01-Serial ;
    sdata:consistsOf ex:NMC811Cathode ,
                     ex:LithiumCarbonate .

# ─── Processes ───────────────────────────────────────────────────────────────
ex:CellManufacturing
    a sdata:Process ;
    sdata:name "Zellherstellung Batch 2026-Q1" ;
    sdata:wasPerformedBy ex:CellChem ;
    sdata:consumes ex:NMC811Cathode ,
                   ex:LithiumCarbonate ;
    sdata:generates ex:Cell-A01 ;
    sdata:locatedAt ex:CellFactory ;
    prov:startedAtTime "2026-01-10T06:00:00Z"^^xsd:dateTimeStamp ;
    prov:endedAtTime "2026-01-12T18:00:00Z"^^xsd:dateTimeStamp .

ex:PackAssemblyProcess
    a sdata:Process ;
    sdata:name "Pack-Montage VP-BAT-2026-000001" ;
    sdata:wasPerformedBy ex:VoltPack ;
    sdata:consumes ex:Module-M01 ;
    sdata:generates ex:BatteryPack-001 ;
    sdata:locatedAt ex:PackAssembly-Hall3 ;
    prov:startedAtTime "2026-01-15T07:00:00Z"^^xsd:dateTimeStamp ;
    prov:endedAtTime "2026-01-15T15:30:00Z"^^xsd:dateTimeStamp .

ex:RecyclingProcess
    a sdata:Process ;
    sdata:name "Hydrometallurgisches Recycling" ;
    sdata:wasPerformedBy ex:RecycleX ;
    sdata:consumes ex:BatteryPack-001 ;
    sdata:generates ex:RecycledNickel ;
    sdata:locatedAt ex:RecyclingYard .

# ─── Roles ───────────────────────────────────────────────────────────────────
ex:ManufacturerRole
    a sdata:Role ;
    sdata:roleType "Manufacturer" ;
    sdata:description "Hersteller des Battery Packs" .

ex:DigitalTwinRole
    a sdata:Role ;
    sdata:roleType "DigitalTwin" ;
    sdata:description "Digitaler Zwilling des Battery Packs" .

ex:ProductPassportRole
    a sdata:Role ;
    sdata:roleType "ProductPassport" ;
    sdata:description "EU Digital Product Passport" .

# ─── Role Realizations ──────────────────────────────────────────────────────
ex:VoltPack sdata:realizes ex:ManufacturerRole .

# ─── Digital Artifacts ───────────────────────────────────────────────────────
ex:BatteryPassport-001
    a sdata:DigitalArtifact ;
    sdata:name "Battery Passport VP-BAT-2026-000001" ;
    sdata:version "1.0" ;
    sdata:description "EU Digital Product Passport gemäß Battery Regulation" ;
    sdata:represents ex:BatteryPack-001 ;
    sdata:realizes ex:ProductPassportRole ;
    sdata:validFrom "2026-01-15T08:00:00Z"^^xsd:dateTimeStamp .

ex:BatteryTwin-001
    a sdata:DigitalArtifact ;
    sdata:name "Digital Twin VP-BAT-2026-000001" ;
    sdata:version "2.1" ;
    sdata:represents ex:BatteryPack-001 ;
    sdata:realizes ex:DigitalTwinRole ;
    sdata:hostedOn ex:BatteryPack-001 .

# =============================================================================
# END — Battery Passport Example
# =============================================================================
